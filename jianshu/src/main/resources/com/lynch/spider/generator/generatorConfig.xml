<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry
		location="C:/m2/repository/mysql/mysql-connector-java/5.1.34/mysql-connector-java-5.1.34.jar" />
	<context id="context">
		<!-- <plugin type="org.mybatis.generator.plugins.ToStringPlugin" /> -->
		<commentGenerator>
			<!-- 阻止自动生成注释 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/jianshu"
			userId="root" password="root" />
		<javaModelGenerator targetPackage="com.lynch.spider.model"
			targetProject="jianshu" />

		<sqlMapGenerator targetPackage="com.lynch.spider.dao"
			targetProject="jianshu/src/main/resources" />


		<javaClientGenerator targetPackage="com.lynch.spider.dao"
			targetProject="jianshu" type="XMLMAPPER" />

		<table schema="jianshu" tableName="collection" domainObjectName="Collection" />
		<table schema="jianshu" tableName="article" domainObjectName="Article" />

	</context>
</generatorConfiguration>